{
  "version": 2,
  "builds": [
    {
      "src": "angular.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "config": {
      "budgets": [
        {
          "type": "size",
          "resource": "esm",
          "limit": "2000 KB"
        }
      ]
    }
  }
}
